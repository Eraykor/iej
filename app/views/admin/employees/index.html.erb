<div id="nav">
    <%= link_to "Edit Profile", edit_admin_employee_path(current_user.id) %>
</div>

<p id="notice"><%= notice %></p>

<h1>Listing Admin Employees</h1>

<table>
  <thead>
    <tr>
      <th>Last name</th>
      <th>First name</th>
      <th>Organization</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Id number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_employees.each do |admin_employee| %>
      <tr>
        <td><%= admin_employee.last_name %></td>
        <td><%= admin_employee.first_name %></td>
        <td><%= admin_employee.organization %></td>
        <td><%= admin_employee.phone %></td>
        <td><%= admin_employee.email %></td>
        <td><%= admin_employee.id_number %></td>
        <td><%= link_to 'Show', admin_employee %></td>
        <td><%= link_to 'Edit', edit_admin_employee_path(admin_employee) %></td>
        <td><%= link_to 'Destroy', admin_employee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee', new_admin_employee_path %>
